{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.29/schema.json",
  "app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.yaml",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 2,
  "default_container_padding": 4,
  "transparency": false,
  "border": true,
  "border_style": "Rounded",
  "border_width": 3,
  "border_offset": -1,
  "border_colours": {
    "single": "#e2874d",
    "stack": "#e2874d",
    "monocle": "#e2874d",
    "unfocused": "#000000",
    "floating": "#e2874d"
  },
  "animation": {
    "enabled": false,
    "duration": 150,
    "style": "EaseInCubic",
    "fps": 60
  },
  "stackbar": {
    "height": 30,
    "mode": "Never",
    "tabs": {
      "width": 200,
      "focused_text": "#FFF"
    },
    "label": "Title"
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "Columns"
        },
        {
          "name": "II",
          "layout": "Columns"
        },
        {
          "name": "III",
          "layout": "Columns"
        },
        {
          "name": "IV",
          "layout": "Columns"
        },
        {
          "name": "V",
          "layout": "Columns"
        },
        {
          "name": "VI",
          "layout": "Columns"
        },
        {
          "name": "VII",
          "layout": "Columns"
        },
        {
          "name": "VIII",
          "layout": "Columns"
        },
        {
          "name": "IX",
          "layout": "Columns"
        }
      ]
    }
  ]
}
